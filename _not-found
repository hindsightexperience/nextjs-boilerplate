import React, { useState, useEffect } from "react";

export default function NotFoundPage() {
  const [hasMounted, setHasMounted] = useState(false);

  useEffect(() => {
    setHasMounted(true);
  }, []);

  if (!hasMounted) {
    return null; // This is valid for server-side rendering (SSR) to avoid rendering on the server
  }

  return
   ;
}
